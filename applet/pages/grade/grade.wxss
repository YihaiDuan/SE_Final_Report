.bookpage
{
  margin-top:20rpx;
display:flex;
justify-content:space-between;
flex-wrap:nowrap;
height:200rpx;
white-space:nowrap;

}

page{
background:#F7F7F7;

}
.topimage{
width:100%;
height:400rpx;

}
.package{
border-bottom:1rpx solid #E3E3E3;
background:#fff;
padding:30rpx;
margin-bottom:30rpx;
}
.title{
font-size:34rpx;
font-weight: 600;
margin-bottom:40rpx;
}
.details{
 padding:30rpx;
 border:1rpx solid #E7E7E7;
box-shadow: 2rpx 2rpx 2rpx #E6E6E6;
display:flex;
margin-bottom:20rpx;
}
.leftdetails{
width:160rpx;
border-right:1rpx dotted #333;
font-size:32rpx;
text-align:center;
}
.rightdetails{
flex:1;
display:flex;

}
.leftnews{
    display:flex;
    flex-direction: column;
    padding:0 40rpx;
   flex:1;
}
.open{
    display:inline-block;
 color:#fff;
 background:#007bff;
 font-size:30rpx;
 letter-spacing: 4rpx;
 padding:12rpx 40rpx;
 vertical-align:middle;
 margin-top:16rpx;
}
.privilege{
 
  flex-direction: column;
width:170rpx;
border:1rpx solid #E6E6E6;
margin-left:20rpx;
margin-bottom:20rpx;
padding:18rpx;
text-align: center;

}
.privilegeimages{
width:60rpx;
height:60rpx;



}



.paybg {
  background: rgba(0, 0, 0, 0.5) !important;
  background: #000;
  display: block;
  height: 100%;
  left: 0;
  position: fixed;
  top: 0;
  width: 100%;
  z-index: 99;
}

.payPass {
  background: #F8F8F8;
  display: block;
  height: 470rpx;
  left: 7%;
  margin-top: -160rpx;
  overflow: hidden;
  position: fixed;
  top: 19%;
  width: 86%;
  z-index: 100;
}

.pas-title {
  color: #333;
  font-size: 32rpx;
  padding:8rpx 18rpx;
  margin: 0;
  line-height:75rpx;
  position: relative;
  width: 100%;
  box-sizing: border-box;
 
  font-weight:600;
  border-bottom:1rpx solid #BBE5BD;
}



.pas-num {
  color: #000;
  font-family: 'Segoe UI', Tahoma, Geneva, Verdana, sans-serif;
  font-size: 26rpx;
  padding:16rpx 0;
  margin: 0;
  position: relative;
  text-align: center;
  display:flex;
  flex-direction: column;
  width: 100%;
}


.pas-box {
  display: block;
  height: 100rpx;
  overflow: hidden;
  width: 100%;
  margin-top:18rpx;
}

.box-six {
  display: block;
  height: 95rpx;
  overflow: hidden;
  width: 100%;
}

.six {
  border-bottom: 1rpx solid #ccc;
  border-right: 1rpx solid #ccc;
  border-left: 1rpx solid #ccc;
  border-top: 1rpx solid #ccc;
  height: 90rpx;
  margin: 0 auto;
  overflow: hidden;
  position: relative;
  width: 600rpx;
}

.i {
  border-left: 1rpx solid #eee;
  display: block;
  float: left;
  height: 90rpx;
  /*padding: 10rpx 0 0 -4rpx;*/
  width: 100rpx;
  box-sizing: border-box;
  align-content: center;
  padding-left:18rpx;

}

.i.active { background: url(http://appuat.huihuishenghuo.com/img/detail/blink.gif) no-repeat center center; }

.b {
  background: url(http://appuat.huihuishenghuo.com/img/detail/dot.png) no-repeat center center;
  display: block;
  height: 20rpx;
  margin-top: 36rpx;
  margin-left:6px;
}

.pas-input {
  border: 0;
  color: #fff;
  font-size: 24rpx;
  height: 68rpx;
  left: -200rpx;
  margin: 0;
  opacity: 0;
  overflow: hidden;
  position: absolute;
  width: 720rpx;
  z-index: 8;
  
}
.userimage{
width:60rpx;
height:60rpx;
border-radius: 50%;
margin-left:6rpx;


}
.chahaoimages{
width:38rpx;
height:48rpx;
margin-top:10rpx;

}
.yinhang{
 border-top:1rpx solid #eee;
 border-bottom:1rpx solid #eee;
 margin:0 20rpx;
padding:20rpx 0;
font-size:30rpx;
display:flex;
}
.yinhangimages{
width:60rpx;
height:60rpx;
margin-right:10rpx;
margin-top:-10rpx;


}
